extends layout

block content
    div(class="postBorder")
        nav(class='navBar')
            ul
                each item in config.menu
                    a(href = item[1])
                        div(class='post_item')
                            li(id=item[0]) #{item[0]}
        ul
            each post in posts
                include post.pug
    script
        include vote.js
